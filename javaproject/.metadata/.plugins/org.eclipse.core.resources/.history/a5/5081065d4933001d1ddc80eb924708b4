package homework;

public class Calculator {

	public static void main(String[] args) {
		
		
		
	}
	
	
	public static int add(int sum1, int sum2) {
		int result = sum1 + sum2;
		return result;
	}

	
	public static int minus(int sum1, int sum2) {
		int result;
		if(sum1>=sum2) {
			result = sum1 - sum2;
		}
		else {
			result = sum2 - sum1;
		}
		return result;
	}

	
	public static int multiplication(int sum1, int sum2) {
		int result = sum1 * sum2;
		return result;
	}

	
	public static int division(int sum1, int sum2) {
		int result;
		if(sum1>=sum2) {
			result = sum1 / sum2;
		}
		else {
			result = sum2 / sum1;
		}
		return result;
	}
	
}





