package chapter02;

public class VariableCatingOperator {

	public static void main(String[] args) {
		
		char c1 = 'a';
		int result = c1 + 10; // char + int => int + int => result = int
		
		System.out.println(result);
		
		int num1 = 10;
		float num2 = 10.0f;
		
		float result2 = num1 + num2;
		
		System.out.println(result2);
		
	}
}
